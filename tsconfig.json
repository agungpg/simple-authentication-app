{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["jest"],
    "paths": {
      "@screens/*": ["app/screens/*"],
      "@components/*": ["app/components/*"],
      "@type/*": ["app/types/*"],
      "@storage/*": ["app/storage/*"],
      "@services/*": ["app/services/*"],
      "@validations/*": ["app/validations/*"],
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["**/node_modules", "**/Pods"]
}
